{
  "name": "APAS Extension",
  "displayName": "APAS Extension",
  "version": "0.0.1",
  "description": "An APAS extension for University of Minnesota Students. It aims to help students understand what courses they are required to take and links to past statistics for the course using data from gophergrades(umn.lol)",
  "author": "Adit Kadepurkar",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.84.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": ["https://apas.umn.edu/*"]
  }
}
